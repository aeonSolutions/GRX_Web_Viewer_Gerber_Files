{
  "name": "grx-frontend",
  "version": "1.0.2",
  "private": true,
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "dotenv-cli": "^4.0.0",
    "electron": "^10.3.2",
    "electron-devtools-installer": "^3.0.0",
    "electron-react-devtools": "^0.5.3",
    "foreman": "^3.0.1",
    "react-scripts": "0.8.5"
  },
  "dependencies": {
    "antd": "^4.4.0",
    "axios": "^0.20.0",
    "dotenv": "^8.2.0",
    "electron-packager": "^14.2.1",
    "express": "^4.17.1",
    "gerber-to-svg": "^4.2.1",
    "pcb-stackup": "^4.2.1",
    "react": "^16.1.1",
    "react-color": "^2.18.1",
    "react-dom": "^16.1.1",
    "react-scripts": "0.8.5",
    "serve": "^11.3.2",
    "svg.js": "^2.7.1",
    "three": "^0.120.0"
  },
  "homepage": "./",
  "main": "src/electron.js",
  "scripts": {
    "start": "dotenv -e .env nf start",
    "dev": "dotenv -e .env node src/electron-wait-react",
    "start-react": "dotenv -e .env react-scripts start",
    "start-electron": "electron .",
    "start-server": "serve -s build -l 5000",
    "pack": "build --dir",
    "build": "react-scripts build",
    "build_osx": "electron-packager . GRX --platform=darwin --arch=x64 --overwrite --icon=public/favicon.ico ; echo 'Built for OSX'",
    "build_linux": "electron-packager . GRX --platform=linux  --arch=x64 --overwrite --icon=public/favicon.ico ; echo 'Built for LINUX'",
    "build_win": "electron-packager . GRX --platform=win32  --arch=x64 --overwrite --icon=public/favicon.ico ; echo 'Built for WIN'"
  },
  "author": "Hunter Creery"
}
