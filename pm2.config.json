{
  "apps": [
    {
      "name": "grx-frontend",
      "script": "npx",
      "interpreter": "none",
      "args": "serve -s frontend/build -l 5000",
      "env": {
        "REACT_APP_GRX_API": "http://grx.creery.org",
        "REACT_APP_GRX_APIPORT": "/api"
      }
    },
    {
      "name": "grx-backend",
      "script": "./backend/index.js",
      "instances": 3,
      "env": {
        "ARTWORKDB": "/artworkdb/"
      }
    }
  ]
}
